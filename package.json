{
  "name": "m-pdf",
  "scripts": {
    "start": "webpack serve --open",
    "build": "webpack --env isProd",
    "predeploy": "npm run build --silent",
    "deploy": "gh-pages -d dist",
    "extract": "formatjs extract 'src/**/*.ts*' --out-file lang/extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile lang/extracted/es.json --ast --out-file lang/compiled/es.json"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "clsx": "^1.1.1",
    "core-js": "^3.16.4",
    "pdf-lib": "^1.16.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.4",
    "react-intl": "^5.20.10",
    "react-pdf": "^5.3.2",
    "regenerator-runtime": "^0.13.9",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@formatjs/cli": "^4.2.33",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-pdf": "^5.0.5",
    "@types/uuid": "^8.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-formatjs": "^10.3.8",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  },
  "browserslist": [
    "defaults"
  ]
}
